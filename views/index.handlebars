<div class="container">
    <div class="row">
        <div class="col s12">
            <h3 class="center=align">Keep track of shows you need to binge over quarantine!</h3>
        </div>
        <div class="col s6 m6 l6">
            {{#each shows}}
                {{#unless watched}}
                    <p>Title: {{title}}</p>
                {{/unless}}
            {{/each}}
        </div>
    </div>
</div>